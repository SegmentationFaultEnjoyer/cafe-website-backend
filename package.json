{
  "name": "tsikava",
  "version": "2.0.0",
  "scripts": {
    "dev": "clear && NODE_ENV=development nodemon",
    "build": "rm -rf dist && NODE_ENV=production tsc -p .",
    "start": "pm2 start server/server.js",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "class-validator": "^0.14.0",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
